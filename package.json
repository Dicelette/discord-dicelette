{
  "name": "dicelette",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "repository": "",
  "scripts": {
    "init": "pnpm i && node install.js",
    "prebuild": "rm -rf dist",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "prestart": "pnpm run build",
    "start": "pm2 dist/src/index.js --node-args='-r tsconfig-paths/register' --name dicelette --log-date-format=\"YYYY-MM-DD HH:mm Z\"",
    "start:node": "node dist/src/index.js",
    "stop": "pm2 stop dicelette",
    "prerestart": "pnpm run build",
    "restart": "pm2 restart dicelette",
    "delete": "pm2 delete dicelette",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn -- src/index.ts",
    "dev:prod": "cross-env ENV=production ts-node-dev -r tsconfig-paths/register --respawn -- src/index.ts",
    "lint:fix": "eslint --fix --ext .ts src/"
  },
  "keywords": [],
  "author": "",
  "license": "GNU GPLv3",
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.5.0",
    "@dicelette/core": "^1.1.1",
    "@discordjs/rest": "^2.2.0",
    "@types/parse-color": "^1.0.3",
    "color-parse": "^2.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "i18next": "^23.10.1",
    "mathjs": "^12.4.1",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "nodemon": "^3.1.0",
    "random-js": "^2.1.0",
    "remove-accents": "^0.5.0",
    "ts-dedent": "^2.2.0"
  },
  "devDependencies": {
    "@lisandra-dev/eslint-config": "^1.1.5",
    "ansi-colors": "^4.1.3",
    "chartjs-node-canvas": "^4.1.6",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "discord.js": "^14.14.1",
    "enmap": "^5.9.10",
    "json-colorizer": "^2.2.2",
    "parse-color": "^1.0.0",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.6.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4"
  }
}
