{
  "name": "dicelette",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "repository": "",
  "type": "module",
  "scripts": {
    "init": "pnpm i && node install.js",
    "prebuild": "cross-os clean",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "prestart": "pnpm run build",
    "start": "pm2 start dist/src/index.js --node-args='-r tsconfig-paths/register' --name dicelette --log-date-format=\"YYYY-MM-DD HH:mm Z\"",
    "start:tsx": "pm2 start tsx src/index.ts --name dicelette --log-date-format=\"YYYY-MM-DD HH:mm Z\"",
    "start:node": "node -r tsconfig-paths/register dist/src/index.js",
    "stop": "pm2 stop dicelette",
    "prerestart": "pnpm run build",
    "restart": "pm2 restart dicelette",
    "delete": "pm2 delete dicelette",
    "dev": "tsx watch src/index.ts",
    "lint:fix": "eslint --fix --ext .ts src/",
    "db": "tsx db-management.ts",
    "release": "commit-and-tag-version",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc --project tsconfig.json --skipLibCheck --noEmit && tsc-alias -p tsconfig.json"
  },
  "cross-os": {
    "clean": {
      "linux": "rm -rf dist",
      "win32": "rmdir /s /q dist"
    }
  },
  "commit-and-tag-version": {
    "t": ""
  },
  "keywords": [],
  "author": "",
  "license": "GNU GPLv3",
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.5.0",
    "@dicelette/core": "^1.1.3",
    "@discordjs/rest": "^2.2.0",
    "@types/papaparse": "^5.3.14",
    "@types/parse-color": "^1.0.3",
    "better-serialize": "^1.0.0",
    "chalk": "^4",
    "chart.js": "^3.9.1",
    "color-parse": "^2.0.2",
    "commit-and-tag-version": "^12.4.0",
    "csv-generate": "^4.4.1",
    "discord-api-types": "^0.37.84",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.0",
    "eslint-plugin-json": "^3.1.0",
    "i18next": "^23.10.1",
    "mathjs": "^12.4.1",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.0",
    "papaparse": "^5.4.1",
    "random-js": "^2.1.0",
    "remove-accents": "^0.5.0",
    "ts-dedent": "^2.2.0",
    "vitest-tsconfig-paths": "^3.4.1"
  },
  "devDependencies": {
    "@lisandra-dev/eslint-config": "^1.1.6",
    "ansi-colors": "^4.1.3",
    "chartjs-node-canvas": "^4.1.6",
    "commander": "^12.0.0",
    "cross-env": "^7.0.3",
    "cross-os": "^1.5.0",
    "csv-stringify": "^6.5.0",
    "csv-writer": "^1.6.0",
    "discord.js": "^14.14.1",
    "enmap": "^6.0.2",
    "json-colorizer": "^2.2.2",
    "parse-color": "^1.0.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.6.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}