{
	"name": "@dicelette/bot-core",
	"version": "1.0.0",
	"description": "Core types, interfaces and client for Dicelette Discord bot",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc --outdir dist --project tsconfig.json && tsc-alias -p tsconfig.json",
		"prebuild": "rimraf dist",
		"tsc": "tsc --project tsconfig.json --skipLibCheck --noEmit && tsc-alias -p tsconfig.json"
	},
	"keywords": [
		"discord",
		"bot",
		"core"
	],
	"author": "Lisandra-dev",
	"license": "AGPL-3.0",
	"dependencies": {
		"@dicelette/types": "workspace:*",
		"@dicelette/utils": "workspace:*",
		"discord.js": "^14.16.3",
		"enmap": "^6.0.4"
	},
	"devDependencies": {
		"typescript": "^5.7.2"
	}
}
