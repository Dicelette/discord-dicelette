{
	"name": "@dicelette/bot-helpers",
	"version": "1.0.0",
	"description": "Helper utilities for Dicelette Discord bot",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc --outdir dist --project tsconfig.json && tsc-alias -p tsconfig.json",
		"prebuild": "rimraf dist",
		"tsc": "tsc --project tsconfig.json --skipLibCheck --noEmit && tsc-alias -p tsconfig.json"
	},
	"keywords": [
		"discord",
		"bot",
		"helpers",
		"utilities"
	],
	"author": "Mara-Li",
	"license": "AGPL-3.0",
	"dependencies": {
		"@dicelette/client": "workspace:*",
		"@dicelette/localization": "workspace:*",
		"@dicelette/parse_result": "workspace:*",
		"@dicelette/types": "workspace:*",
		"@dicelette/utils": "workspace:*",
		"discord.js": "^14.16.3"
	},
	"devDependencies": {
		"typescript": "^5.7.2"
	}
}
